#!/bin/bash

CMDNAME=`basename $0`
OUTFILE=$2_https_httponlycookie_$1.txt

if [ $# -ne 2 ]; then
        echo "Usage: $CMDNAME ip port" 1>&2
        exit 1
fi

HEADER1="Accept: text/html, application/xhtml+xml, */*"
HEADER2="Accept-Language: ja-JP"
HEADER3="User-Agent: Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; WOW64; Trident/5.0)"
HEADER4="Accept-Encoding: gzip, deflate"
HEADER5="Connection: close"

echo -e "\r\n\r\n********* HTTPOnlyCookieDisclosure  ********" > $OUTFILE 2>&1
echo -e "# openssl s_client -quiet -connect $1:$2 2>/dev/null\r\nGET / HTTP/1.1\r\nHost: $1\r\nUser-Agent: Mozilla/5.0 (Windows NT 6.1; WOW64; rv:10.0.2) Gecko/20100101 Firefox/10.0.2\r\nAccept: text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8\r\nAccept-Language: ja,en-us;q=0.7,en;q=0.3\r\nAccept-Encoding: gzip, deflate\r\nConnection: close\r\nCookie: SESSIONID=dummycookie12345; ; ; ; ; ; ; ; ; ; \r\n\r\n">> $OUTFILE 2>&1
echo -e "GET / HTTP/1.1\r\nHost: $1\r\nUser-Agent: Mozilla/5.0 (Windows NT 6.1; WOW64; rv:10.0.2) Gecko/20100101 Firefox/10.0.2\r\nAccept: text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8\r\nAccept-Language: ja,en-us;q=0.7,en;q=0.3\r\nAccept-Encoding: gzip, deflate\r\nConnection: close\r\nCookie: SESSIONID=dummycookie12345; ; ; ; ; ; ; ; ; ; \r\n\r\n" | openssl s_client -quiet -connect $1:$2 >> $OUTFILE 2>&1

echo "  **** HTTP check completed. ****"
exit 0
