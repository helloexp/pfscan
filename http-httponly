#!/bin/bash

CMDNAME=`basename $0`
LOGPATH="./log/"
#OUTFILE="$LOGPATH$RESULT"

if [ $# -ne 3 ]; then
    echo "#### Error ####"
    echo "Usage: $CMDNAME {ipaddress} {port} [http or https]" 1>&2
    echo "Example: $CMDNAME 192.168.1.1 443 https"
    exit 1
fi

if [ ! -d $LOGPATH ]; then
    mkdir $LOGPATH
fi

if [ $3 = "http" ]; then
    COMMAND="nc $1 $2"
    CURL="curl -isk http://$1:$2"
    RESULT="$2_http_httponly_$1_`date "+%Y%m%d%H%M"`.txt"
    OUTFILE="$LOGPATH$RESULT"
fi

if [ $3 = "https" ]; then
    COMMAND="openssl s_client -connect $1:$2"
    CURL="curl -isk https://$1:$2"
    RESULT="$2_https_httponly_$1_`date "+%Y%m%d%H%M"`.txt"
    OUTFILE="$LOGPATH$RESULT"
fi

HDR1="User-Agent: Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; WOW64; Trident/5.0)"
HDR2="Connection: close"

COOKIE="; ; ; ; ; ; ; ; ; "

echo -e "# $COMMAND\r\nGET / HTTP/1.1\r\nHost: $1\r\n$HDR1\r\n$HDR2\r\nCookie: SESSIONID=dummycookie12345; $COOKIE\r\n\r\n" > $OUTFILE 2>&1
RESPONSE=`echo -e "GET / HTTP/1.1\r\nHost: $1\r\n$HDR1\r\n$HDR2\r\nCookie: SESSIONID=dummycookie12345; $COOKIE\r\n\r\n" | $COMMAND2` 2>&1
echo $RESPONSE  >> $OUTFILE 2>&1

if echo "$RESPONSE" | grep "z1=AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" > /dev/null; then
    echo "---- VULNERABLE ----" | tee $RESULT
    echo -e "\r\n# $COMMAND\r\nGET / HTTP/1.1\r\nHost: $1\r\n$HDR1\r\n$HDR2\r\nCookie: SESSIONID=dummycookie12345; $COOKIE\r\n\r\n" >> $RESULT 2>&1
    echo "$RESPONSE" >> $RESULT
else
    echo "---- NOT VULNERABLE ----" | tee $RESULT
fi

exit 0
