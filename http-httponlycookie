#!/bin/bash

CMDNAME=`basename $0`
OUTFILE=$2_http_onlycookie_$1.txt

if [ $# -ne 2 ]; then
        echo "Usage: $CMDNAME ip port" 1>&2
        exit 1
fi

HDR1="User-Agent: Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; WOW64; Trident/5.0)"
HDR2="Connection: close"

echo -e "\r\n\r\n********* HTTPOnlyCookieDisclosure  ********" > $OUTFILE 2>&1
echo -e "# nc $1 $2\r\nGET / HTTP/1.1\r\nHost: $1\r\n$HDR1\r\n$HDR2\r\nCookie: SESSIONID=dummycookie12345; ; ; ; ; ; ; ; ; ; \r\n\r\n">> $OUTFILE 2>&1
echo -e "GET / HTTP/1.1\r\nHost: $1\r\n$HDR1\r\n$HDR2\r\nCookie: SESSIONID=dummycookie12345; ; ; ; ; ; ; ; ; ; \r\n\r\n" | nc $1 $2 >> $OUTFILE 2>&1

echo "  **** HTTP check completed. ****"
exit 0
